{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/Nova-Committee/McBot"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "cn.evole.mods.mcbot.fabric.McBotFabric"
        ],
        "modmenu": [
            "cn.evole.mods.mcbot.fabric.ModMenuApiImpl"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "depends": {
        "fabric-api": "*",
        "minecraft": "${minecraft_version}"
    }
}
  